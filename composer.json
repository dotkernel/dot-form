{
  "name": "dotkernel/dot-form",
  "description": "DotKernel form component based on zend-form",
  "authors": [
    {
      "name": "Popa Tiberiu",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "form",
    "input filter",
    "services"
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-form": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Form\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Form\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
