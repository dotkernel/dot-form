{
    "name": "dotkernel/dot-form",
    "description": "DotKernel form component extending and customizing laminas-form",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "form",
        "input filter",
        "services"
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.5",
        "doctrine/annotations": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Form\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Form\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        }
    }
}
