{
  "name": "dotkernel/dot-form",
  "description": "DotKernel form component extending and customizing zend-form",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "form",
    "input filter",
    "services"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-form": "^2.9",

    "dotkernel/dot-hydrator": "^1.0",
    "dotkernel/dot-filter": "^1.0",
    "dotkernel/dot-validator": "^1.0",
    "dotkernel/dot-inputfilter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5",
    "doctrine/annotations": "^1.3",

    "dotkernel/dot-mapper": "~0.1"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Form\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Form\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
