{
  "name": "dotkernel/dot-form",
  "description": "DotKernel form component based on zend-form",
  "authors": [
    {
      "name": "Popa Tiberiu",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "form",
    "input filter",
    "services"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-form": "^2.9",

    "dotkernel/dot-hydrator": "0.6.x-dev",
    "dotkernel/dot-filter": "0.6.x-dev",
    "dotkernel/dot-validator": "0.6.x-dev",
    "dotkernel/dot-inputfilter": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5",
    "doctrine/annotations": "^1.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-inputfilter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-filter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-validator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-hydrator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dot\\Form\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Form\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
